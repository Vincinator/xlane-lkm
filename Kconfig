#
# Sassy subsystem configuration
#

menuconfig ASGUARD
	bool "Enable ASGUARD"
	default y
	help
	  This enables the network submodule ASGUARD

config ASGUARD_FOLLOWER_TEST
        bool "Follower test"
        depends on ASGUARD && KUNIT

config ASGUARD_NET_TEST
        bool "Net Helper test"
        depends on ASGUARD && KUNIT
